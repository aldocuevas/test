	<queryString language="SQL">
		<![CDATA[SELECT
     clientes.`id` AS clientes_id,
     clientes.`nombre` AS clientes_nombre,
     clientes.`apellidoPaterno` AS clientes_apellidoPaterno,
     clientes.`apellidoMaterno` AS clientes_apellidoMaterno,
     clientes.`calle` AS clientes_calle,
     clientes.`numero` AS clientes_numero,
     clientes.`numeroInterior` AS clientes_numeroInterior,
     clientes.`calleAledanaA` AS clientes_calleAledanaA,
     clientes.`calleAledanaB` AS clientes_calleAledanaB,
     clientes.`colonia` AS clientes_colonia,
     clientes.`telefono` AS clientes_telefono,
     clientes.`celular` AS clientes_celular,
     clientes.`fechaNacimiento` AS clientes_fechaNacimiento,
     clientes.`tallaCalzado` AS clientes_tallaCalzado,
     clientes.`tallaConjunto` AS clientes_tallaConjunto,
     clientes.`tieneCredito` AS clientes_tieneCredito,
     clientes.`limiteCredito` AS clientes_limiteCredito,
     clientes.`estadoCivil` AS clientes_estadoCivil,
     clientes.`casaPropia` AS clientes_casaPropia,
     clientes.`trabaja` AS clientes_trabaja,
     clientes.`ocupacion` AS clientes_ocupacion,
     clientes.`domicilioTrabajo` AS clientes_domicilioTrabajo,
     clientes.`telefonoTrabajo` AS clientes_telefonoTrabajo,
     clientes.`nombreEmpresa` AS clientes_nombreEmpresa,
     clientes.`nombreConyugue` AS clientes_nombreConyugue,
     clientes.`ocupacionConyugue` AS clientes_ocupacionConyugue,
     clientes.`nombreEmpresaConyugue` AS clientes_nombreEmpresaConyugue,
     clientes.`domicilioTrabajoConyugue` AS clientes_domicilioTrabajoConyugue,
     clientes.`personal1` AS clientes_personal1,
     clientes.`personal2` AS clientes_personal2,
     clientes.`comercial1` AS clientes_comercial1,
     clientes.`comercial2` AS clientes_comercial2,
     clientes.`comercial3` AS clientes_comercial3,
     clientes.`familiar1` AS clientes_familiar1,
     clientes.`familiar2` AS clientes_familiar2,
     clientes.`claveCliente` AS clientes_claveCliente,
     clientes.`clasificacion` AS clientes_clasificacion,
     colonias.`ruta` AS colonias_ruta
FROM
     `colonias` colonias INNER JOIN `clientes` clientes ON colonias.`nombre` = clientes.`colonia`
ORDER BY
     colonias.`ruta` ASC]]>
	</queryString>
